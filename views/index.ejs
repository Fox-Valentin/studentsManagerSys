<!DOCTYPE html>
<html lang="en">
  <head>
    <title></title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <style>
      table, th, tr, td {
        border:1px solid #ccc;
      }
    </style>
  </head>
  <body>
  <table>
    <tr>
      <th>sid</th>
      <th>name</th>
      <th>gender</th>
      <th>course</th>
    </tr>
    <% for(var i = 0; i < students.length; i++) { %>
    <tr>
      <td><%=students[i].sid%></td>
      <td><%=students[i].name%></td>
      <td><%=students[i].gender%></td>
      <td><%=students[i].course%></td>
    </tr>
    <% } %>
  </table>
  </body>
</html>